{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "installCommand": "pip install -r requirements.txt"
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/simulate",
      "dest": "api/index.py/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}